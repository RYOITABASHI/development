modules = ["nodejs-20"]

[nix]
channel = "stable-24_05"

[workspace]
recentFiles = ["src/main.tsx", "vite.config.ts", "package.json"]

[deployment]
run = "cd mention-test/VEGETA && npx vite --host 0.0.0.0 --port 5173"
deploymentTarget = "static"

[[ports]]
localPort = 5173
externalPort = 80

[env]
NODE_ENV = "development"
VITE_DEV_MODE = "true"
REPLIT_ENV = "true"